{
    "options": {
        "downloadBase": "/var/lib/mongodb-mms-automation"
    },
    "mongoDbVersions": [
        {
            "name": "2.6.9-ent"
        }
    ],
    "backupVersions": [
        {
            "hostname": "AGENT_HOSTNAME"
        }
    ],

    "monitoringVersions": [
        {
            "hostname": "AGENT_HOSTNAME"
        }
    ],
    "processes": [
        {
            "args2_6": {
                "net": {
                    "port": 28000
                },
                "replication": {
                    "replSetName": "blue"
                },
                "storage": {
                    "dbPath": "/data/blue_0"
                },
                "systemLog": {
                    "destination": "file",
                    "path": "/data/blue_0/mongodb.log"
                },
                "setParameter": {
                    "saslauthdPath": "/var/run/saslauthd/mux"
                }
            },
            "hostname": "AGENT_HOSTNAME",
            "logRotate": {
                "sizeThresholdMB": 1000,
                "timeThresholdHrs": 24
            },
            "name": "blue_0",
            "processType": "mongod",
            "version": "2.6.9-ent",
            "authSchemaVersion": 3
        },
        {
            "args2_6": {
                "net": {
                    "port": 28001
                },
                "replication": {
                    "replSetName": "blue"
                },
                "storage": {
                    "dbPath": "/data/blue_1"
                },
                "systemLog": {
                    "destination": "file",
                    "path": "/data/blue_1/mongodb.log"
                },
                "setParameter": {
                    "saslauthdPath": "/var/run/saslauthd/mux"
                }
            },
            "hostname": "AGENT_HOSTNAME",
            "logRotate": {
                "sizeThresholdMB": 1000,
                "timeThresholdHrs": 24
            },
            "name": "blue_1",
            "processType": "mongod",
            "version": "2.6.9-ent",
            "authSchemaVersion": 3
        },
        {
            "args2_6": {
                "net": {
                    "port": 28002
                },
                "replication": {
                    "replSetName": "blue"
                },
                "storage": {
                    "dbPath": "/data/blue_2"
                },
                "systemLog": {
                    "destination": "file",
                    "path": "/data/blue_2/mongodb.log"
                },
                "setParameter": {
                    "saslauthdPath": "/var/run/saslauthd/mux"
                }
            },
            "hostname": "AGENT_HOSTNAME",
            "logRotate": {
                "sizeThresholdMB": 1000,
                "timeThresholdHrs": 24
            },
            "name": "blue_2",
            "processType": "mongod",
            "version": "2.6.9-ent",
            "authSchemaVersion": 3
        }
    ],
    "replicaSets": [
        {
            "_id": "blue",
            "members": [
                {
                    "_id": 0,
                    "host": "blue_0",
                    "priority": 5
                },
                {
                    "_id": 1,
                    "host": "blue_1"
                },
                {
                    "_id": 2,
                    "arbiterOnly": true,
                    "host": "blue_2",
                    "priority": 0
                }
            ]
        }
    ],
    "roles": [],
    "sharding": [],
    "auth": {
        "disabled": false,
        "autoAuthMechanism": "PLAIN",
        "autoUser": "automation-agent",
        "autoPwd": "apassword",
        "deploymentAuthMechanisms": ["PLAIN"],
        "key": "someverylongandrandomcontents",
        "keyfile": "/var/lib/mongodb-mms-automation/keyfile",
        "usersWanted": [
             {
                "db": "$external",
                "roles": [
                    {
                        "db": "admin",
                        "role": "clusterMonitor"
                    }
                ],
                "user": "monitoring-agent"
            },
            {
                "db": "$external",
                "roles": [
                    {
                        "db": "admin",
                        "role": "clusterAdmin"
                    },
                    {
                        "db": "admin",
                        "role": "readAnyDatabase"
                    },
                    {
                        "db": "admin",
                        "role": "userAdminAnyDatabase"
                    },
                    {
                        "db": "local",
                        "role": "readWrite"
                    },
                    {
                        "db": "admin",
                        "role": "readWrite"
                    }
                ],
                "user": "backup-agent"
            }
        ],
        "usersDeleted": []
    }
}
