                    "replSetName": "blue"
                },
                "storage": {
                    "dbPath": "/data/blue_1"
                },
                "systemLog": {
                    "destination": "file",
                    "path": "/data/blue_1/mongodb.log"
                }
            },
            "hostname": "AGENT_HOSTNAME",
            "logRotate": {
                "sizeThresholdMB": 1000,
                "timeThresholdHrs": 24
            },
            "name": "blue_1",
            "processType": "mongod",
