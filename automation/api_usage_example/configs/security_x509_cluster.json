{
    "options": {
        "downloadBase": "/var/lib/mongodb-mms-automation"
    },
    "mongoDbVersions": [
        {
            "name": "2.6.9-ent"
        }
    ],
    "backupVersions": [
        {
            "hostname": "AGENT_HOSTNAME"
        }
    ],

    "monitoringVersions": [
        {
            "hostname": "AGENT_HOSTNAME"
        }
    ],
    "processes": [
        {
