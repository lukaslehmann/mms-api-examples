                "net": {
                    "port": 28000,
                    "ssl": {
                        "mode": "requireSSL",
                        "PEMKeyFile": "/opt/mongodb-1.pem"
                    }
                },
                "replication": {
                    "replSetName": "blue"
                },
                "storage": {
                    "dbPath": "/data/blue_0"
                },
                "systemLog": {
                    "destination": "file",
                    "path": "/data/blue_0/mongodb.log"
                }
            },
            "hostname": "AGENT_HOSTNAME",
            "logRotate": {
                "sizeThresholdMB": 1000,
                "timeThresholdHrs": 24
            },
            "name": "blue_0",
            "processType": "mongod",
            "version": "2.6.9-ent",
            "authSchemaVersion": 3
        },
        {
            "args2_6": {
                "net": {
                    "port": 28001,
