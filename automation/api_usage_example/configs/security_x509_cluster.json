                    }
                },
                "replication": {
                    "replSetName": "blue"
                },
                "storage": {
                    "dbPath": "/data/blue_2"
                },
                "systemLog": {
                    "destination": "file",
                    "path": "/data/blue_2/mongodb.log"
                }
            },
            "hostname": "AGENT_HOSTNAME",
            "logRotate": {
                "sizeThresholdMB": 1000,
                "timeThresholdHrs": 24
            },
            "name": "blue_2",
            "processType": "mongod",
            "version": "2.6.9-ent",
