            "version": "2.6.9-ent",
            "authSchemaVersion": 3
        }
    ],
    "replicaSets": [
        {
            "_id": "blue",
            "members": [
                {
                    "_id": 0,
                    "host": "blue_0",
                    "priority": 5
                },
                {
                    "_id": 1,
                    "host": "blue_1"
                },
                {
                    "_id": 2,
                    "arbiterOnly": true,
                    "host": "blue_2",
                    "priority": 0
                }
            ]
        }
    ],
    "roles": [],
    "sharding": [
        {
            "configServer": [
                "blue_config_4"
            ],
            "name": "blueCluster",
            "shards": [
                {
                    "_id": "blue",
                    "rs": "blue"
                }
            ]
        }
    ],
    "auth": {
        "disabled": false,
        "autoAuthMechanism": "MONGODB-X509",
        "autoUser": "CN=automation-agent,O=MMS",
        "deploymentAuthMechanisms": ["MONGODB-X509"],
        "key": "someverylongandrandomcontents",
        "keyfile": "/var/lib/mongodb-mms-automation/keyfile",
        "usersWanted": [
             {
                "db": "$external",
                "roles": [
                    {
                        "db": "admin",
                        "role": "clusterMonitor"
                    }
                ],
                "user": "CN=monitoring-agent,O=MMS"
            },
            {
                "db": "$external",
                "roles": [
                    {
                        "db": "admin",
                        "role": "clusterAdmin"
                    },
                    {
                        "db": "admin",
                        "role": "readAnyDatabase"
                    },
                    {
                        "db": "admin",
                        "role": "userAdminAnyDatabase"
                    },
                    {
                        "db": "local",
                        "role": "readWrite"
                    },
                    {
                        "db": "admin",
                        "role": "readWrite"
                    }
                ],
                "user": "CN=backup-agent,O=MMS"
            }
        ],
        "usersDeleted": []
    },
    "ssl": {
        "CAFilePath": "/opt/ca.pem",
        "clientCertificateMode": "REQUIRE",
        "autoPEMKeyFilePath": "/opt/automation-agent.pem"

    }
}
