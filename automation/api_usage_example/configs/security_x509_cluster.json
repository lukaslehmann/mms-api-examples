        {
            "args2_6": {
                "net": {
                    "port": 28019,
                    "ssl": {
                        "mode": "requireSSL",
                        "PEMKeyFile": "/opt/mongodb-1.pem"
                    }
                },
                "sharding": {
                    "clusterRole": "configsvr"
                },
                "storage": {
                    "dbPath": "/data/blue_config_4"
